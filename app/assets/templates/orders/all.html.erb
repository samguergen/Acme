<h2>List of All Orders</h2>


<table style="width:100%">
  <tr>
    <td>Order ID</td>
    <td>Name</td>
    <td>Valid?</td>
  </tr>
</table>

<div class="orders-list" ng-repeat="ord in allOrderz">

<table style="width:100%">
  <tr>
    <td>{{ ord['order_id'] }}</td>
    <td>{{ ord['name'] }}</td>
    <td>{{ ord['valid'] }}</td>
  </tr>
</table>



</div>